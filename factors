import sys

def factorize(num):
    for i in range(2, num):
        if num % i == 0:
            return (i, num//i)
    return None

if len(sys.argv) != 2:
    print("Usage: factors <file>")
    sys.exit(1)

filename = sys.argv[1]
try:
    with open(filename, 'r') as f:
        for line in f:
            num = int(line.strip())
            factors = factorize(num)
            if factors is not None:
                print(f"{num}={factors[0]}*{factors[1]}")
except FileNotFoundError:
    print(f"Error: {filename} not found")
    sys.exit(1)
