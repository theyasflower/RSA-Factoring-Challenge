import sys
import math

def is_prime(num):
    if num < 2:
        return False
    for i in range(2, int(math.sqrt(num)) + 1):
        if num % i == 0:
            return False
    return True

def factorize(num):
    for i in range(2, int(math.sqrt(num)) + 1):
        if num % i == 0 and is_prime(i) and is_prime(num // i):
            return (i, num // i)
    return None

if len(sys.argv) != 2:
    print("Usage: rsa_factors <file>")
    sys.exit(1)

filename = sys.argv[1]
try:
    with open(filename, 'r') as f:
        num = int(f.readline().strip())
        factors = factorize(num)
        if factors is not None:
            print(f"{num}={factors[0]}*{factors[1]}")
        else:
            print(f"{num} is prime")
except FileNotFoundError:
    print(f"Error: {filename} not found")
    sys.exit(1)
